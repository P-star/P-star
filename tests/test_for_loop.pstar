SCENE main {
	struct person {
		string name;
		int age;
		person(string _name, int _age) {
			name = _name;
			age = _age;
		};
		public int print() {
			echo "Name: " . name . ", Age: " . age . "\n";
		};
	};

	array<person> persons;

	persons[@persons]("A", 10);
	persons[@persons]("B", 20);
	persons[@persons]("C", 30);
	persons[@persons]("D", 40);
	persons[@persons]("E", 50);

	int i;
	for (i = 0; i < @persons; i++) {
		echo "Entry " . i . " - ";
		persons[i]->print();
	}

	return 0;
}
